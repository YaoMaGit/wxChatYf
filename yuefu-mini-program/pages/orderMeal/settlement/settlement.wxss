.container {
  padding-bottom: 96rpx;
}

.top-tabs {
  height: 80rpx;
  line-height: 80rpx;
  background: #161D3A;
  position: relative;
  display: flex;
  align-items: flex-end;
}
.top-tabs .box {
  display: flex;
  font-size: 30rpx;
  color: #333333;
  position: absolute;
  left: 0;
  top: 12rpx;
  width: 100%;
}
.top-tabs .box>view {
  height: 68rpx;
  line-height: 68rpx;
  flex: 1;
  text-align: center;
}
.top-tabs .tabs-bg {
  height: 68rpx;
  width: calc(100% - 32rpx);
  background: #d2d3d5;
  margin-left: 16rpx;
}
.top-tabs .tabs-bg>image {
  height: 80rpx;
  margin-top: -12rpx;
  transform: rotateY(180deg);
}
.top-tabs .tabs-bg>image.rotate {
  transform: rotateY(0deg);
}

.item-box {
  width: calc(100% - 32rpx);
  margin: 0 auto;
  background: #fff;
  margin-bottom: 8rpx;
}
.addr-info {
  position: relative;
  border-bottom: 1px solid #E8E8E8;
  padding: 28rpx 62rpx 14rpx 8rpx;
  margin: 0 20rpx;
}
/* .addr-info>view:nth-child(1) {
  font-size: 28rpx;
  color: #777777;
  line-height: 1;
  display: flex;
  align-items: flex-end;
}
.addr-info>view:nth-child(1)>image {
  width: 26rpx;
