}

.popup {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.4);
  z-index: 10;
}
.coupons-popup .pic {
  position: absolute;
  width: 100%;
  top: 0;
  left: 18rpx;
  height: 100vh;
}

.good-info {
  position: absolute;
  width: calc(100% - 40rpx);
  height: calc(100vh - 80rpx);
  top: 40rpx;
  left: 20rpx;
  background: #F9F9F9;
  border-radius: 4rpx;
  overflow-y: auto;
}
.good-detail .close-btn {
  position: absolute;
  top: 54rpx;
  right: 34rpx;
  width: 46rpx;
  height: 46rpx;
  z-index: 11;
}
.good-info>view {
  margin-bottom: 10rpx;
  background: #fff;
  padding: 20rpx;
}
.good-info .info-box {
  padding: 20rpx 20rpx 34rpx 20rpx;
}
.good-info .info-box .pic {
