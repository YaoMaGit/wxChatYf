}

.banjia-box {
  padding: 20rpx 20rpx 0rpx 20rpx;
  display: flex;
  background: #fff;
  height:346rpx;
  overflow: hidden;
}
.banjia-box image {
  display: block;
}
.banjia-box .item {
  flex: 1;
}
.banjia-box .left {
  position: relative;
}
.banjia-box .left .info {
  position: absolute;
  top: 22rpx;
  left: 24rpx;
}
.banjia-box .left .info>view:nth-child(1) {
  color: #FF7858;
  font-size: 26rpx;
}
.banjia-box .left .info>view:nth-child(2) {
  font-size: 22rpx;
  color: #666666;
  margin: 20rpx 0 14rpx 0;
}
.banjia-box .left .info>view:nth-child(3) {
  display: flex;
  font-size: 22rpx;
}
.banjia-box .left .info>view:nth-child(3)>view {
  width: 34rpx;
  height: 30rpx;
  color: #FFFFFF;
  background: #070707;
  border-radius: 6rpx;
  text-align: center;
}
.banjia-box .left .info .price {
  width: 108rpx;
  height: 84rpx;
  background: #FA5C37;
  border-radius: 50%;
  color: #FFFFFF;
  text-align: center;
  font-size: 18rpx;
  padding-top: 24rpx;
}
.banjia-box .left .info .price>view {
  line-height: 1;
}
.banjia-box .left .info .price>view:nth-child(1) {
  font-size: 16rpx;
}
.banjia-box .left .info .price .text-2 {
  font-size: 30rpx;
  font-weight: bold;
  line-height: 1;
}
.banjia-box .right {
  margin-left: 10rpx;
}
.banjia-box .left image {
  height: 346rpx;
}
.banjia-box .right>view {
  height: 168rpx;
  position: relative;
}
.banjia-box .right>view:first-child {
  margin-bottom: 10rpx;
}
.banjia-box .right>view .info {
  position: absolute;
  top: 22rpx;
  left: 10rpx;
}
.banjia-box .right>view .info>view:nth-child(1) {
  color: #FFC122;
  font-size: 26rpx;
  line-height: 1;
}
.banjia-box .right>view .info>view:nth-child(2) {
  color: #666666;
  font-size: 22rpx;
  margin: 12rpx 0;
  line-height: 1;
}
.banjia-box .right>view .info>view:nth-child(3) {
  width: 74rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 22rpx;
  color: #FFFFFF;
  background: #FA5C37;
  border-radius: 14rpx;
  text-align: center;
}
.good-box {
