}
.toutiao-box .right {
  width: 154rpx;
}
.toutiao-box .right image {
  height: 90rpx;
}

.jiri-box {
  height: 264rpx;
  background: #fff;
  padding: 0 24rpx;
  margin-bottom: 10rpx;
}
.jiri-box .top {
  height: 76rpx;
  line-height: 76rpx;
}
.jiri-box .top text {
  line-height: 1;
}
.jiri-box .top>text:nth-child(1) {
  color: #666666;
  font-size: 32rpx;
  font-weight: bold;
}
.jiri-box .top>text:nth-child(2) {
  color: #FFC122;
  font-size: 22rpx;
